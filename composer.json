{
    "name": "jdenoc/laravel-app-version",
    "description": "artisan app:version",
    "keywords": [
        "jdenoc",
        "laravel",
        "artisan",
        "version"
    ],
    "homepage": "https://github.com/jdenoc/laravel-app-version",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Jdenoc\\LaravelAppVersion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jdenoc\\LaravelAppVersion\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jdenoc\\LaravelAppVersion\\AppVersionServiceProvider"
            ]
        }
    }
}
